{
    "version": 2,
    "name": "website-prog-futuro",
    "builds": [{
        "src": "package.json",
        "use": "@now/static-build"
    }],
    "alias": [
        "www.programadoresdofuturo.org",
        "programadoresdofuturo.org"
    ],
    "routes": [
        {
            "src": "programadoresdofuturo.org(.*)",
            "status": 301,
            "headers": {
                "Location": "https://www.programadoresdofuturo.org$1"
            }
        }
    ]
}
